plugins {
    id 'java'
}

sourceCompatibility = 1.8
targetCompatibility = 1.8

repositories {
    mavenLocal()
    maven {url 'https://biger-exchange.github.io/biger-client/'}
    mavenCentral()
}

dependencies {
    compile group: "com.biger", name: "biger-client-core", version: bigerClientVersion
    compile group: "com.biger", name: "biger-client-httpops-httpurlconnection", version: bigerClientVersion
}
